{
  "expo": {
    "name": "QuizApp",
    "slug": "QuizApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/quiz.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/quiz.png",
      "resizeMode": "contain",
      "backgroundColor": "#fef4e8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ibrahimdevv.QuizApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/quiz.png",
        "backgroundColor": "#fef4e8"
      },
      "package": "com.ibrahimdevv.QuizApp"
    },
    "web": {
      "favicon": "./assets/quiz.png"
    }
  }
}
